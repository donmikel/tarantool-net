<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
  </configSections>
  <akka>
    <hocon>
      <![CDATA[
          tarantool{
              connection{
                timeout = 2s

                #Delay before trying to reconnect
                retry.delay = 1 s

                #Number of connect attempts before failure
                retry.attempts = 3
              }
          }
      ]]>
    </hocon>
  </akka>
